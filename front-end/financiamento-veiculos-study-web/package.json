{
  "name": "financiamento-veiculos-study-web",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --host 0.0.0.0",
    "start:dev": "ng serve --host 0.0.0.0 --port 4200 --configuration=dev",
    "start:mock": "concurrently --kill-others \"npm run mock:server\" \"npm run start:dev\"",
    "build": "ng build --build-optimizer=true --aot=true --vendor-chunk=true --progress=true",
    "test": "jest --env=jsdom",
    "test:coverage": "jest --coverage --env=jsdom",
    "test:watch": "jest --watch --env=jsdom",
    "test:updateSnapshot": "jest --env=jsdom --updateSnapshot",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "mock:server": "nodemon src/assets/mock/json-server-run.js",
    "sonar": "sonar-scanner -Dsonar.host.url=http://127.0.0.1:9000 -Dsonar.login=admin -Dsonar.password=admin -Dproject.settings=sonar-project.properties"
  },
  "private": true,
  "dependencies": {
    "@angular-builders/jest": "^10.0.0",
    "@angular/animations": "^10.0.8",
    "@angular/common": "~10.0.4",
    "@angular/compiler": "~10.0.4",
    "@angular/core": "~10.0.4",
    "@angular/forms": "~10.0.4",
    "@angular/platform-browser": "~10.0.4",
    "@angular/platform-browser-dynamic": "~10.0.4",
    "@angular/router": "~10.0.4",
    "@types/jasmine": "^3.5.12",
    "@types/jasminewd2": "^2.0.8",
    "@types/jest": "^26.0.9",
    "angular-locale-pt-br": "^1.3.16",
    "bootstrap": "^4.5.0",
    "jasmine-core": "^3.6.0",
    "jasmine-spec-reporter": "^5.0.2",
    "jest": "^26.4.0",
    "jest-preset-angular": "^8.2.1",
    "ngx-bootstrap": "^5.6.1",
    "ngx-mask": "^9.1.2",
    "ngx-toastr": "^13.0.0",
    "rxjs": "~6.5.5",
    "tslib": "^2.0.0",
    "zone.js": "~0.10.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1000.3",
    "@angular/cli": "~10.0.4",
    "@angular/compiler-cli": "~10.0.4",
    "@types/node": "^12.11.1",
    "codelyzer": "^6.0.0",
    "protractor": "~7.0.0",
    "sonar-scanner": "^3.1.0",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.0",
    "typescript": "~3.9.5"
  }
}
